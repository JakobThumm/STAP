<?xml version="1.0" ?>

<robot name="cracker">

  <material name="color">
    <color rgba=".6 0 0 1"/>
  </material>

  <link name="base">

    <contact>
      <lateral_friction value="1.0"/>
      <!-- <spinning_friction value="0.001"/> -->
      <!-- <rolling_friction value="0.001"/> -->
    </contact>

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="0.1"/>
       <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size=".05 .05 .1"/>
      </geometry>
      <material name="color"/>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size=".05 .05 .1"/>
      </geometry>
    </collision>

  </link>

</robot>
